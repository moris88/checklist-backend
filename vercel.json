    "devCommand": "pnpm serve",
    "outputDirectory": "dist",
    "framework": null,
    "public": true,
    "version": 2,
    "env": {
        "BLOB_READ_WRITE_TOKEN": "vercel_blob_rw_MuSYUeogPrKAISOA_USJ45lMrG0ZjF6khiK25KaeSQiJl92",
